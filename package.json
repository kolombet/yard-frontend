{
  "name": "yard-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "normalize.css": "^7.0.0",
    "pluralize-ru": "^1.0.1",
    "qs": "^6.5.0",
    "react": "^15.6.1",
    "react-body-classname": "^1.2.0",
    "react-dom": "^15.6.1",
    "react-flexbox-grid": "^1.1.3",
    "react-router-dom": "^4.1.1",
    "react-scripts": "1.0.10",
    "shortid": "^2.2.8",
    "styled-components": "^2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js --ext jsx src/",
    "prepush": "yarn lint && yarn flow",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "deploy": "yarn build && now build --public --name yard-frontend && now alias"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.50.0",
    "flow-typed": "^2.1.5",
    "husky": "^0.14.3",
    "now": "^7.2.4"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/prop-types": "off"
    },
    "env": {
      "browser": true,
      "jest": true
    }
  },
  "now": {
    "alias": "kolombet-yard-frontend"
  }
}
